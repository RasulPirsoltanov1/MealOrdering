
@code {
    [Parameter]
    public string Message { get; set; }
    [Parameter]
    public string OkText { get; set; }
    [Parameter]
    public string CancelText { get; set; }
    [Parameter]
    public string YesText { get; set; }
    [Parameter]
    public string NoText { get; set; }
    [CascadingParameter]
    BlazoredModalInstance BlazoredModalInstance { get; set; }

    public async Task ConfirmClick()
    {
        await BlazoredModalInstance.CloseAsync(ModalResult.Ok<string>(""));
    }
    public async Task CancelClick()
    {
        await BlazoredModalInstance.CloseAsync(ModalResult.Cancel<string>(""));
    }
}
